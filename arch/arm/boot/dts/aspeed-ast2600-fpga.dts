/dts-v1/;

#include "aspeed-g6.dtsi"

/ {
	model = "AST2600 FPGA";
	compatible = "aspeed,ast2600";

	aliases {
		serial0 = &uart1;
		serial1 = &uart2;
		serial2 = &uart3;
		serial3 = &uart4;
		serial4 = &uart5;
		serial5 = &uart6;
		serial6 = &uart7;
		serial7 = &uart8;
		serial8 = &uart9;
		serial9 = &uart10;
		serial10 = &uart11;
		serial11 = &uart12;
		serial12 = &uart13;
	};

	chosen {
		stdout-path = &uart5;
		bootargs = "console=ttyS4,115200n8 root=/dev/ram rw init=/linuxrc";
	};

	cpus {
		CPU0: cpu@0 {
			clock-frequency = <0x2FAF080>;
		};

		CPU1: cpu@1 {
			clock-frequency = <0x2FAF080>;
		};
	};

	timer {
		clock-frequency = <0x17d7840>;
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x40000000>;
	};

	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		gfx_memory: framebuffer {
			size = <0x01000000>;
			alignment = <0x01000000>;
			compatible = "shared-dma-pool";
			reusable;
		};

		video_memory: video {
			size = <0x04000000>;
			alignment = <0x01000000>;
			compatible = "shared-dma-pool";
			no-map;
		};

		ssp_memory: ssp_memory {
			size = <0x00200000>;
			alignment = <0x00100000>;
			compatible = "shared-dma-pool";
			no-map;
		};

	};
};

&uart1 {
	status = "okay";
};

&uart2 {
	status = "okay";
};

&uart3 {
	status = "okay";
};

&mdio0 {
	status = "okay";

	ethphy0: ethernet-phy@0 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0>;
	};
};

&mac0 {
	status = "okay";
	phy-mode = "rgmii";
	phy-handle = <&ethphy0>;
};

&fmc {
	status = "okay";
	flash@0 {
		status = "okay";
		m25p,fast-read;
		label = "bmc";
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			u-boot@0 {
				reg = <0x0 0x90000>;
				label = "u-boot";
			};
			
			u-boot-env@e0000 {
				reg = <0x90000 0x10000>;
				label = "u-boot-env";
			};

			fit@100000 {
				reg = <0xa0000 0x1f60000>;
				label = "fit";
			};
		};
	};
};

&i3c0 {
	status = "okay";
	jdec-spd;
	/* IDT IMX3102 multiplexer w/ thermal sensor */
	imx3102: i3cmux@0f,3C000000000 {
		reg = <0x0f 0x3C0 0x00000000>;
		assigned-address = <0x0f>;
	};

	/* Renesas SPD5118 */
	spd5118: spd@50,3C000000001 {
		reg = <0x50 0x3C0 0x00000001>;
		assigned-address = <0x50>;
	};
};