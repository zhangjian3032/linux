&pcie {
	status = "okay";

	interrupts = <GIC_SPI 168 IRQ_TYPE_LEVEL_HIGH>;

	pcie_intc0: legacy-interrupt-controller {
		interrupts = <GIC_SPI 168 IRQ_TYPE_EDGE_RISING>;
	};
};

&h2x {
	status = "okay";
};

#if 1
&video {
	status = "okay";
	memory-region = <&video_engine_memory>;
};
#else
&rvas {
	status = "okay";
	memory-region = <&rvas_memory>;
};
#endif


&i3c0 {
	status = "okay";
	jdec-spd;
	/* IDT IMX3102 multiplexer w/ thermal sensor */
	imx3102: i3cmux@0f,3C000000000 {
		reg = <0x0f 0x3C0 0x00000000>;
		assigned-address = <0x0f>;
	};

	/* Renesas SPD5118 */
	spd5118: spd@50,3C000000001 {
		reg = <0x50 0x3C0 0x00000001>;
		assigned-address = <0x50>;
	};
};

&syscon {
	mac0-clk-delay = <0x1d 0x1b
			  0x10 0x17
			  0x10 0x17>;
	mac1-clk-delay = <0x1d 0x10
			  0x10 0x10
			  0x10 0x10>;
	mac2-clk-delay = <0x0a 0x04
			  0x08 0x04
			  0x08 0x04>;
	mac3-clk-delay = <0x0e 0x04
			  0x08 0x04
			  0x08 0x04>;
};

&ssp {
	memory-region = <&ssp_memory>;
};
