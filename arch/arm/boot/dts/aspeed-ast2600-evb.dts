&fmc {
	status = "okay";
	flash@0 {
		status = "okay";
		m25p,fast-read;
		label = "bmc";
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			u-boot@0 {
				reg = <0x0 0x90000>;
				label = "u-boot";
			};

			u-boot-env@e0000 {
				reg = <0x90000 0x10000>; 
				label = "u-boot-env";
			};

			fit@100000 {
				reg = <0xa0000 0x1f60000>;
				label = "fit";
			};

		};
	};
	
	flash@1 {
                status = "okay";
                m25p,fast-read;
                label = "fmc0:1";
                spi-max-frequency = <50000000>;
        };

	flash@2 {
                status = "okay";
                m25p,fast-read;
                label = "fmc0:2";
                spi-max-frequency = <50000000>;
        };
};

&spi1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_spi1_default &pinctrl_spi1cs1_default>;

	flash@0 {
		status = "okay";
		m25p,fast-read;
		label = "spi1:0";
		spi-max-frequency = <50000000>;
	};
	
	flash@1 {
